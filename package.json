{
  "name": "plink-back",
  "version": "0.0.0",
  "private": true,
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js",
      "*.Spec.js"
    ]
  },
  "scripts": {
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/bin/www.js",
    "dev": "npx npm-run-all build server",
    "prod": "SET NODE_ENV=production & npx npm-run-all build server",
    "transpile": "npx babel ./server --out-dir dist-server --copy-files",
    "clean": "npx rimraf dist-server",
    "watch:dev": "npx nodemon",
    "test": "npx jasmine \"dist-server/src/spec/**/*Spec.js\""
  },
  "dependencies": {
    "bcrypt": "^4.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "sequelize": ">=4.44.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "jasmine": "^3.5.0",
    "nodemon": "^1.18.4",
    "sequelize-cli": "^5.5.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
